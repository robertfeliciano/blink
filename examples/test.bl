class Point { 
    let x: i32;
    let y: i32;

    fun sumX(other: Point) => i32 {
        return this.x + other.x;
    }

    fun midpt(other: Point) => Point {
        return new Point {
            x = (this.x + other.x)/2,
            y = (this.y + other.y)/2
        };
    }
}

